      <guid isPermaLink="false">/general-conference/1994/10/miracles-of-the-restoration</guid>
      <enclosure url="http://media2.ldscdn.org/assets/general-conference/october-1994-general-conference/1994-10-2070-elder-jeffrey-r-holland-64k-eng.mp3" length="8798612" type="audio/mpeg"/>
