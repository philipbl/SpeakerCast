      <guid isPermaLink="false">/general-conference/1995/10/this-do-in-remembrance-of-me</guid>
      <enclosure url="http://media2.ldscdn.org/assets/general-conference/october-1995-general-conference/1995-10-4040-elder-jeffrey-r-holland-64k-eng.mp3" length="9105692" type="audio/mpeg"/>
